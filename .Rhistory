teamName <- "Run-N-Gun"
teamInfo <- function(data, teamName) {
teamMeans <- data %>% select(-c(FTM.A., FGM.A.)) %>%
mutate(Week=as.numeric(Week)) %>%
filter(Week > (maxWeeks - recentWeeks)) %>%
group_by(Team) %>%
filter(Team == {{teamName}})
teamMeans
}
oppTeam <- teamInfo(dat, teamName = teamName)
homeTeam <-teamInfo(dat, teamName = "Gian Vesely")
teamCompare <- bind_rows(oppTeam, homeTeam)
diffTable <- homeTeam[,c(2:10)] -oppTeam[,c(2:10)]
plot(diffTable)
teamLongData <- function(data) {
oppTeamLong <- data %>%
pivot_longer(!Team, names_to = "Category", values_to = "count") %>%
arrange(Category) %>% filter(!Category %in% c("Score", "Week", "Season"))
oppTeamLong
}
compareTeams <- teamLongData(teamCompare)
teamPlot <- function(data, teamName) {
ggplot(compareTeams, aes(x = Category, y = count)) +
geom_point(alpha = .1, shape = 1) + facet_wrap(vars(Category), scales = "free", nrow = 1) +
geom_point(data = data %>% filter(Team == teamName), color = "red", size = 2) +
geom_point(data = data %>% filter(Team == "Gian Vesely"), color = "turquoise", size = 2) +
theme_bw()
}
teamPlot(compareTeams, teamName)
