dat$Team <- gsub( "Bam!|logoThy Bulles","Ohhhh Herro!", dat$Team)
dat$Team <- gsub("logoBlack Falcon", "ClarenceWeatherspoon", dat$Team)
dat$Team <- gsub("Glasgow Rocks","KB Tueta Durres", dat$Team)
dat$Team <- gsub("logo", "", dat$Team)
dat <- dat %>% filter(Season == 2021)
# team <- dat %>% select(-c(FTM.A., FGM.A.)) %>% mutate(Week=as.numeric(Week)) %>% group_by(Team) %>% summarise_all(list(median))
dat$`FG.` <- as.numeric(gsub("\\*", "", dat$`FG.`))
# list the median score for 3-pt baskets
recentWeeks <- 3
maxWeeks <- max(dat$Week)
teamMedians <- dat %>% select(-c(FTM.A., FGM.A.)) %>%
mutate(Week=as.numeric(Week)) %>%
filter(Week > (maxWeeks - recentWeeks)) %>%
group_by(Team) %>%
summarise_all(list(median))
# rank(team$X3PTM)
vars <- c("FG.", "FT.", "X3PTM", "PTS", "REB",
"AST", "ST", "BLK", "TO")
rankEm <- function(data, variable, reverse = FALSE) {
if(variable %in% c("TO")) {
rank <- as.vector(rank(data[,variable]))
} else {
rank <- as.vector(rank(-data[,variable]))
}
rank
}
teamRanks <- as.data.frame(teamMedians)
for(i in vars) {
teamRanks[,i] <- rankEm(teamRanks, i, reverse = TRUE)
}
